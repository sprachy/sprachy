    "assert": "^2.1.0",
    "better-sqlite3": "^9.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.19.13",
    "env-cmd": "^10.1.0",
    "glob": "^10.3.10",
    "miniflare": "^2.13.0",
    "npm-upgrade": "^3.1.0",
    "nuxt": "^3.8.0",
